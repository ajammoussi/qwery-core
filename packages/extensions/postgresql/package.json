{
  "name": "@qwery/extension-postgresql",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/driver.js",
  "types": "./dist/driver.d.ts",
  "exports": {
    ".": {
      "types": "./dist/driver.d.ts",
      "default": "./dist/driver.js"
    }
  },
  "scripts": {
    "lint": "echo \"no lint configured\"",
    "test": "vitest run --coverage --passWithNoTests",
    "build": "tsc -p tsconfig.build.json"
  },
  "contributes": {
    "datasources": [
      {
        "id": "postgresql",
        "name": "PostgreSQL",
        "description": "Connect to PostgreSQL databases",
        "icon": "media/postgresql.svg",
        "schema": {
          "type": "object",
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "host": {
                  "type": "string",
                  "description": "PostgreSQL server hostname"
                },
                "port": {
                  "type": "number",
                  "description": "PostgreSQL server port",
                  "default": 5432
                },
                "username": {
                  "type": "string",
                  "description": "PostgreSQL username"
                },
                "user": {
                  "type": "string",
                  "description": "PostgreSQL username (alias for username)"
                },
                "password": {
                  "type": "string",
                  "description": "PostgreSQL password",
                  "format": "password"
                },
                "database": {
                  "type": "string",
                  "description": "PostgreSQL database name"
                },
                "sslmode": {
                  "type": "string",
                  "enum": [
                    "disable",
                    "require",
                    "prefer",
                    "verify-ca",
                    "verify-full"
                  ],
                  "description": "SSL mode",
                  "default": "prefer"
                }
              },
              "required": [
                "host"
              ]
            },
            {
              "type": "object",
              "properties": {
                "connectionUrl": {
                  "type": "string",
                  "description": "PostgreSQL connection string (postgresql://user:pass@host:port/db)"
                }
              },
              "required": [
                "connectionUrl"
              ]
            }
          ]
        },
        "drivers": [
          "postgresql.default"
        ]
      },
      {
        "id": "postgresql-supabase",
        "name": "Supabase",
        "description": "Connect to Supabase databases",
        "icon": "media/supabase-logo-wordmark--dark.svg",
        "schema": {
          "type": "object",
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "host": {
                  "type": "string",
                  "description": "Supabase server hostname"
                },
                "port": {
                  "type": "number",
                  "description": "Supabase server port",
                  "default": 5432
                },
                "username": {
                  "type": "string",
                  "description": "Supabase username"
                },
                "user": {
                  "type": "string",
                  "description": "Supabase username (alias for username)"
                },
                "password": {
                  "type": "string",
                  "description": "Supabase password",
                  "format": "password"
                },
                "database": {
                  "type": "string",
                  "description": "Supabase database name"
                },
                "sslmode": {
                  "type": "string",
                  "enum": [
                    "disable",
                    "require",
                    "prefer",
                    "verify-ca",
                    "verify-full"
                  ],
                  "description": "SSL mode",
                  "default": "prefer"
                }
              },
              "required": [
                "host"
              ]
            },
            {
              "type": "object",
              "properties": {
                "connectionUrl": {
                  "type": "string",
                  "description": "Supabase connection string (postgresql://user:pass@host:port/db)"
                }
              },
              "required": [
                "connectionUrl"
              ]
            }
          ]
        },
        "drivers": [
          "postgresql.default"
        ]
      },
      {
        "id": "postgresql-neon",
        "name": "Neon",
        "description": "Connect to Neon databases",
        "icon": "media/neon-logo-dark-color.svg",
        "schema": {
          "type": "object",
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "host": {
                  "type": "string",
                  "description": "Neon server hostname"
                },
                "port": {
                  "type": "number",
                  "description": "Neon server port",
                  "default": 5432
                },
                "username": {
                  "type": "string",
                  "description": "Neon username"
                },
                "user": {
                  "type": "string",
                  "description": "Neon username (alias for username)"
                },
                "password": {
                  "type": "string",
                  "description": "Neon password",
                  "format": "password"
                },
                "database": {
                  "type": "string",
                  "description": "Neon database name"
                },
                "sslmode": {
                  "type": "string",
                  "enum": [
                    "disable",
                    "require",
                    "prefer",
                    "verify-ca",
                    "verify-full"
                  ],
                  "description": "SSL mode",
                  "default": "prefer"
                }
              },
              "required": [
                "host"
              ]
            },
            {
              "type": "object",
              "properties": {
                "connectionUrl": {
                  "type": "string",
                  "description": "Neon connection string (postgresql://user:pass@host:port/db)"
                }
              },
              "required": [
                "connectionUrl"
              ]
            }
          ]
        },
        "drivers": [
          "postgresql.default"
        ]
      }
    ],
    "drivers": [
      {
        "id": "postgresql.default",
        "name": "PostgreSQL (Node)",
        "description": "Default Node runtime PostgreSQL driver",
        "runtime": "node",
        "entry": "./dist/driver.js"
      }
    ]
  },
  "dependencies": {
    "@qwery/extensions-sdk": "workspace:*",
    "pg": "^8.13.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/pg": "^8.11.10",
    "@vitest/coverage-istanbul": "catalog:",
    "vitest": "catalog:"
  }
}
