{
  "name": "@qwery/extension-clickhouse-web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/driver.js",
  "types": "./dist/driver.d.ts",
  "exports": {
    ".": {
      "types": "./dist/driver.d.ts",
      "default": "./dist/driver.js"
    }
  },
  "scripts": {
    "lint": "echo \"no lint configured\"",
    "test": "vitest run --coverage --passWithNoTests",
    "build": "tsc -p tsconfig.build.json"
  },
  "contributes": {
    "datasources": [
      {
        "id": "clickhouse-web",
        "name": "ClickHouse (Browser)",
        "description": "Connect to ClickHouse databases",
        "icon": "media/ClickHouseLogo_White_FNL.svg",
        "schema": {
          "type": "object",
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "host": {
                  "type": "string",
                  "description": "ClickHouse server hostname"
                },
                "port": {
                  "type": "number",
                  "description": "ClickHouse server port",
                  "default": 8123
                },
                "username": {
                  "type": "string",
                  "description": "ClickHouse username",
                  "default": "default"
                },
                "user": {
                  "type": "string",
                  "description": "ClickHouse username (alias for username)",
                  "default": "default"
                },
                "password": {
                  "type": "string",
                  "description": "ClickHouse password",
                  "format": "password"
                },
                "database": {
                  "type": "string",
                  "description": "ClickHouse database name",
                  "default": "default"
                }
              },
              "required": [
                "host"
              ]
            },
            {
              "type": "object",
              "properties": {
                "connectionUrl": {
                  "type": "string",
                  "description": "ClickHouse connection URL (clickhouse://user:pass@host:port/database or http://host:port)",
                  "format": "uri"
                }
              },
              "required": [
                "connectionUrl"
              ]
            }
          ]
        },
        "drivers": [
          "clickhouse.web"
        ]
      }
    ],
    "drivers": [
      {
        "id": "clickhouse.web",
        "name": "ClickHouse (Browser)",
        "description": "Browser runtime ClickHouse driver",
        "runtime": "browser",
        "entry": "./dist/driver.js"
      }
    ]
  },
  "dependencies": {
    "@clickhouse/client-web": "^0.2.10",
    "@qwery/extensions-sdk": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "catalog:",
    "vitest": "catalog:"
  }
}
