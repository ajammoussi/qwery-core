{
  "datasources": [
    {
      "id": "clickhouse-node",
      "name": "ClickHouse (Node)",
      "description": "Connect to ClickHouse databases",
      "icon": "/extensions/clickhouse-node/ClickHouseLogo_White_FNL.svg",
      "schema": {
        "type": "object",
        "properties": {
          "connectionUrl": {
            "type": "string",
            "description": "ClickHouse connection URL (clickhouse://user:pass@host:port/database or http://host:port). Alternative to separate fields.",
            "format": "uri"
          },
          "host": {
            "type": "string",
            "description": "ClickHouse host (e.g., 'localhost' or 'http://localhost:8123'). Required if connectionUrl is not provided."
          },
          "port": {
            "type": "integer",
            "description": "ClickHouse port (default: 8123 for HTTP, 9000 for native). Only used if host is a hostname without protocol.",
            "minimum": 1,
            "maximum": 65535
          },
          "username": {
            "type": "string",
            "description": "ClickHouse username (default: 'default')"
          },
          "password": {
            "type": "string",
            "description": "ClickHouse password (default: empty)",
            "format": "password"
          },
          "database": {
            "type": "string",
            "description": "ClickHouse database name (default: 'default')"
          }
        },
        "oneOf": [
          {
            "required": [
              "connectionUrl"
            ]
          },
          {
            "required": [
              "host"
            ]
          }
        ]
      },
      "packageName": "@qwery/extension-clickhouse-node",
      "drivers": [
        {
          "id": "clickhouse.node",
          "name": "ClickHouse (Node)",
          "description": "Node runtime ClickHouse driver",
          "runtime": "node"
        }
      ]
    },
